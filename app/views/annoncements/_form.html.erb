<%= form_for(@annoncement) do |f| %>
  <div class="field">
    <%= f.label :title , "标题"%><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :content, "内容" %><br />
    <%= f.text_area :content %>
  </div>
  <div class="actions">
    <%= f.submit "提交"%>
  </div>
<% end %>
